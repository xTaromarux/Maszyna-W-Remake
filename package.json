{
  "name": "w",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve:ws": "node server.cjs",
    "dev:ws": "concurrently -k \"npm:serve:ws\" \"npm:dev\"",
    "dev": "vite",
    "dev:esp": "vite --mode esp",
    "build": "npm run build:parsers && vite build",
    "build:esp": "npm run build:parsers && vite build --mode esp",
    "build:parsers": "npm run build:maszynaW && npm run build:macroW",
    "build:maszynaW": "lezer-generator src/codemirror-langs/maszynaW.grammar -o src/codemirror-langs/maszynaW.js",
    "build:macroW": "lezer-generator src/codemirror-langs/macroW.grammar -o src/codemirror-langs/macroW.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.12.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/language": "^6.10.0",
    "@codemirror/lint": "^6.5.0",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.4.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.23.0",
    "@lezer/generator": "^1.7.0",
    "@lezer/highlight": "^1.2.0",
    "@lezer/lr": "^1.4.0",
    "codemirror": "^6.0.1",
    "netlify": "^21.5.0",
    "vue": "^3.5.13",
    "vue-codemirror": "^6.1.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "concurrently": "^9.1.2",
    "cypress": "^13.17.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "typescript": "^5.8.3",
    "vite": "^6.0.5",
    "vite-plugin-monaco-editor-esm": "^2.0.2",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8"
  }
}
